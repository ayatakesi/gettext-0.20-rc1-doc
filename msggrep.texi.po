#: original_texis/msggrep.texi:203
#, no-wrap
msgid "--no-location"
msgstr "--no-location"

#. type: opindex
#: original_texis/msggrep.texi:204
#, no-wrap
msgid "--no-location@r{, @code{msggrep} option}"
msgstr "--no-location@r{, @code{msggrep} option}"

#. type: table
#: original_texis/msggrep.texi:206
msgid "Do not write @samp{#: @var{filename}:@var{line}} lines."
msgstr "@samp{#: @var{filename}:@var{line}}という行を書き込みません。"

#. type: item
#: original_texis/msggrep.texi:207
#, no-wrap
msgid "-n"
msgstr "-n"

#. type: itemx
#: original_texis/msggrep.texi:208
#, no-wrap
msgid "--add-location=@var{type}"
msgstr "--add-location=@var{type}"

#. type: opindex
#: original_texis/msggrep.texi:209
#, no-wrap
msgid "--add-location@r{, @code{msggrep} option}"
msgstr "--add-location@r{, @code{msggrep} option}"

#. type: table
#: original_texis/msggrep.texi:211
msgid "Generate @samp{#: @var{filename}:@var{line}} lines (default)."
msgstr "@samp{#: @var{filename}:@var{line}}という行を生成します(デフォルト)。"

#. type: table
#: original_texis/msggrep.texi:217
msgid "The optional @var{type} can be either @samp{full}, @samp{file}, or @samp{never}.  If it is not given or @samp{full}, it generates the lines with both file name and line number.  If it is @samp{file}, the line number part is omitted.  If it is @samp{never}, it completely suppresses the lines (same as @code{--no-location})."
msgstr "@var{type}はオプションで、@samp{full}、@samp{file}、または@samp{never}を指定できます。オプションが指定されない、または@samp{full}の場合は、ファイル名と行番号のの両方が生成されます。@samp{file}の場合、行番号は省略されます。@samp{never}の場合は、完全にこの行を抑制します(@code{--no-location}と同じです)。"

#. type: item
#: original_texis/msggrep.texi:218
#, no-wrap
msgid "--strict"
msgstr "--strict"

#. type: opindex
#: original_texis/msggrep.texi:219
#, no-wrap
msgid "--strict@r{, @code{msggrep} option}"
msgstr "--strict@r{, @code{msggrep} option}"

#. type: table
#: original_texis/msggrep.texi:223
msgid "Write out a strict Uniforum conforming PO file.  Note that this Uniforum format should be avoided because it doesn't support the GNU extensions."
msgstr "Uniforumに厳密に準拠したPOファイルを出力します。このUniforum形式はGNUの拡張をサポートしないため避けたほうがよいでしょう。"

#. type: item
#: original_texis/msggrep.texi:224
#, no-wrap
msgid "-p"
msgstr "-p"

