#. type: pindex
#: original_texis/xgettext.texi:5
#, no-wrap
msgid "xgettext"
msgstr "xgettext"

#. type: cindex
#: original_texis/xgettext.texi:6
#, no-wrap
msgid "@code{xgettext} program, usage"
msgstr "@code{xgettext} program, usage"

#. type: example
#: original_texis/xgettext.texi:9
#, no-wrap
msgid "xgettext [@var{option}] [@var{inputfile}] @dots{}\n"
msgstr "xgettext [@var{option}] [@var{inputfile}] @dots{}\n"

#. type: Plain text
#: original_texis/xgettext.texi:13
msgid "The @code{xgettext} program extracts translatable strings from given input files."
msgstr "@code{xgettext}プログラムは、与えられた入力ファイルから、翻訳可能な文字列を抽出します。"

#. type: subsection
#: original_texis/xgettext.texi:14
#, no-wrap
msgid "Input file location"
msgstr "Input file location"

#. type: item
#: original_texis/xgettext.texi:17
#, no-wrap
msgid "@var{inputfile} @dots{}"
msgstr "@var{inputfile} @dots{}"

#. type: table
#: original_texis/xgettext.texi:19
msgid "Input files."
msgstr "入力ファイルを指定します。"

#. type: item
#: original_texis/xgettext.texi:20
#, no-wrap
msgid "-f @var{file}"
msgstr "-f @var{file}"

#. type: itemx
#: original_texis/xgettext.texi:21
#, no-wrap
msgid "--files-from=@var{file}"
msgstr "--files-from=@var{file}"

#. type: opindex
#: original_texis/xgettext.texi:22
#, no-wrap
msgid "-f@r{, @code{xgettext} option}"
msgstr "-f@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:23
#, no-wrap
msgid "--files-from@r{, @code{xgettext} option}"
msgstr "--files-from@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:26
msgid "Read the names of the input files from @var{file} instead of getting them from the command line."
msgstr "入力ファイルの名前を、コマンドラインからではなく、@var{file}から読み込みます。"

#. type: item
#: original_texis/xgettext.texi:27
#, no-wrap
msgid "-D @var{directory}"
msgstr "-D @var{directory}"

#. type: itemx
#: original_texis/xgettext.texi:28
#, no-wrap
msgid "--directory=@var{directory}"
msgstr "--directory=@var{directory}"

#. type: opindex
#: original_texis/xgettext.texi:29
#, no-wrap
msgid "-D@r{, @code{xgettext} option}"
msgstr "-D@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:30
#, no-wrap
msgid "--directory@r{, @code{xgettext} option}"
msgstr "--directory@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:34
msgid "Add @var{directory} to the list of directories.  Source files are searched relative to this list of directories.  The resulting @file{.po} file will be written relative to the current directory, though."
msgstr "ディレクトリーのリストに@var{directory}を追加します。このディレクトリーのリストからソースファイルを検索します。しかし@file{.po}ファイルが出力されるのは、カレントディレクトリーです。"

#. type: Plain text
#: original_texis/xgettext.texi:38
msgid "If @var{inputfile} is @samp{-}, standard input is read."
msgstr "@var{inputfile}に@samp{-}が指定された場合は、標準入力から読み込みます。"

#. type: subsection
#: original_texis/xgettext.texi:39
#, no-wrap
msgid "Output file location"
msgstr "Output file location"

#. type: item
#: original_texis/xgettext.texi:42
#, no-wrap
msgid "-d @var{name}"
msgstr "-d @var{name}"

#. type: itemx
#: original_texis/xgettext.texi:43
#, no-wrap
msgid "--default-domain=@var{name}"
msgstr "--default-domain=@var{name}"

#. type: opindex
#: original_texis/xgettext.texi:44
#, no-wrap
msgid "-d@r{, @code{xgettext} option}"
msgstr "-d@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:45
#, no-wrap
msgid "--default-domain@r{, @code{xgettext} option}"
msgstr "--default-domain@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:47
msgid "Use @file{@var{name}.po} for output (instead of @file{messages.po})."
msgstr "出力ファイルとして、(@file{messages.po}のかわりに)@file{@var{name}.po}を使用します。"

#. type: item
#: original_texis/xgettext.texi:48
#, no-wrap
msgid "-o @var{file}"
msgstr "-o @var{file}"

#. type: itemx
#: original_texis/xgettext.texi:49
#, no-wrap
msgid "--output=@var{file}"
msgstr "--output=@var{file}"

#. type: opindex
#: original_texis/xgettext.texi:50
#, no-wrap
msgid "-o@r{, @code{xgettext} option}"
msgstr "-o@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:51
#, no-wrap
msgid "--output@r{, @code{xgettext} option}"
msgstr "--output@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:54
msgid "Write output to specified file (instead of @file{@var{name}.po} or @file{messages.po})."
msgstr "(@file{@var{name}.po}や@file{messages.po}ではなく)指定されたファイルに出力を書き込みます。"

#. type: item
#: original_texis/xgettext.texi:55
#, no-wrap
msgid "-p @var{dir}"
msgstr "-p @var{dir}"

#. type: itemx
#: original_texis/xgettext.texi:56
#, no-wrap
msgid "--output-dir=@var{dir}"
msgstr "--output-dir=@var{dir}"

#. type: opindex
#: original_texis/xgettext.texi:57
#, no-wrap
msgid "-p@r{, @code{xgettext} option}"
msgstr "-p@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:58
#, no-wrap
msgid "--output-dir@r{, @code{xgettext} option}"
msgstr "--output-dir@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:60
msgid "Output files will be placed in directory @var{dir}."
msgstr "ファイルは@var{dir}に出力されます。"

#. type: cindex
#: original_texis/xgettext.texi:63
#, no-wrap
msgid "output to stdout, @code{xgettext}"
msgstr "output to stdout, @code{xgettext}"

#. type: Plain text
#: original_texis/xgettext.texi:66
msgid "If the output @var{file} is @samp{-} or @samp{/dev/stdout}, the output is written to standard output."
msgstr "出力の@var{file}に@samp{-}または@samp{/dev/stdout}が指定された場合、出力は標準出力に書き込まれます。"

#. type: subsection
#: original_texis/xgettext.texi:67
#, no-wrap
msgid "Choice of input file language"
msgstr "Choice of input file language"

#. type: item
#: original_texis/xgettext.texi:70
#, no-wrap
msgid "-L @var{name}"
msgstr "-L @var{name}"

#. type: itemx
#: original_texis/xgettext.texi:71
#, no-wrap
msgid "--language=@var{name}"
msgstr "--language=@var{name}"

#. type: opindex
#: original_texis/xgettext.texi:72
#, no-wrap
msgid "-L@r{, @code{xgettext} option}"
msgstr "-L@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:73
#, no-wrap
msgid "--language@r{, @code{xgettext} option}"
msgstr "--language@r{, @code{xgettext} option}"

#. type: cindex
#: original_texis/xgettext.texi:74
#, no-wrap
msgid "supported languages, @code{xgettext}"
msgstr "supported languages, @code{xgettext}"

#. type: table
#: original_texis/xgettext.texi:82
msgid "Specifies the language of the input files.  The supported languages are @code{C}, @code{C++}, @code{ObjectiveC}, @code{PO}, @code{Shell}, @code{Python}, @code{Lisp}, @code{EmacsLisp}, @code{librep}, @code{Scheme}, @code{Smalltalk}, @code{Java}, @code{JavaProperties}, @code{C#}, @code{awk}, @code{YCP}, @code{Tcl}, @code{Perl}, @code{PHP}, @code{GCC-source}, @code{NXStringTable}, @code{RST}, @code{RSJ}, @code{Glade}, @code{Lua}, @code{JavaScript}, @code{Vala}, @code{GSettings}, @code{Desktop}."
msgstr "Specifies the language of the input files.  The supported languages are @code{C}、@code{C++}、@code{ObjectiveC}、@code{PO}、@code{Shell}、@code{Python}、@code{Lisp}、@code{EmacsLisp}、@code{librep}、@code{Scheme}、@code{Smalltalk}、@code{Java}、@code{JavaProperties}、@code{C#}、@code{awk}、@code{YCP}、@code{Tcl}、@code{Perl}、@code{PHP}、@code{GCC-source}、@code{NXStringTable}、@code{RST}、@code{RSJ}、@code{Glade}、@code{Lua}、@code{JavaScript}、@code{Vala}、@code{GSettings}、@code{Desktop}。"

#. type: item
#: original_texis/xgettext.texi:83
#, no-wrap
msgid "-C"
msgstr "-C"

#. type: itemx
#: original_texis/xgettext.texi:84
#, no-wrap
msgid "--c++"
msgstr "--c++"

#. type: opindex
#: original_texis/xgettext.texi:85
#, no-wrap
msgid "-C@r{, @code{xgettext} option}"
msgstr "-C@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:86
#, no-wrap
msgid "--c++@r{, @code{xgettext} option}"
msgstr "--c++@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:88
msgid "This is a shorthand for @code{--language=C++}."
msgstr "@code{--language=C++}の省略指定です。"

#. type: Plain text
#: original_texis/xgettext.texi:93
msgid "By default the language is guessed depending on the input file name extension."
msgstr "デフォルトでは、入力ファイルの言語は拡張子により推測されます。"

#. type: subsection
#: original_texis/xgettext.texi:94
#, no-wrap
msgid "Input file interpretation"
msgstr "Input file interpretation"

#. type: item
#: original_texis/xgettext.texi:97
#, no-wrap
msgid "--from-code=@var{name}"
msgstr "--from-code=@var{name}"

#. type: opindex
#: original_texis/xgettext.texi:98
#, no-wrap
msgid "--from-code@r{, @code{xgettext} option}"
msgstr "--from-code@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:103
msgid "Specifies the encoding of the input files.  This option is needed only if some untranslated message strings or their corresponding comments contain non-ASCII characters.  Note that Tcl and Glade input files are always assumed to be in UTF-8, regardless of this option."
msgstr "入力ファイルのエンコーディングを指定します。このオプションはメッセージ文字列や、それらのコメントに非ASCII文字が含まれている場合のみ必要です。TclとGladeの入力ファイルは、このオプションの指定に関わらず、UTF-8が想定されることに注意してください。"

#. type: Plain text
#: original_texis/xgettext.texi:107
msgid "By default the input files are assumed to be in ASCII."
msgstr "デフォルトでは、入力ファイルのエンコーディングはASCIIであると仮定されます。"

#. type: subsection
#: original_texis/xgettext.texi:108
#, no-wrap
msgid "Operation mode"
msgstr "Operation mode"

#. type: item
#: original_texis/xgettext.texi:111
#, no-wrap
msgid "-j"
msgstr "-j"

#. type: itemx
#: original_texis/xgettext.texi:112
#, no-wrap
msgid "--join-existing"
msgstr "--join-existing"

#. type: opindex
#: original_texis/xgettext.texi:113
#, no-wrap
msgid "-j@r{, @code{xgettext} option}"
msgstr "-j@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:114
#, no-wrap
msgid "--join-existing@r{, @code{xgettext} option}"
msgstr "--join-existing@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:116
msgid "Join messages with existing file."
msgstr "既存のファイルのメッセージを結合します。"

#. type: item
#: original_texis/xgettext.texi:117
#, no-wrap
msgid "-x @var{file}"
msgstr "-x @var{file}"

#. type: itemx
#: original_texis/xgettext.texi:118
#, no-wrap
msgid "--exclude-file=@var{file}"
msgstr "--exclude-file=@var{file}"

#. type: opindex
#: original_texis/xgettext.texi:119
#, no-wrap
msgid "-x@r{, @code{xgettext} option}"
msgstr "-x@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:120
#, no-wrap
msgid "--exclude-file@r{, @code{xgettext} option}"
msgstr "--exclude-file@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:123
msgid "Entries from @var{file} are not extracted.  @var{file} should be a PO or POT file."
msgstr "@var{file}のエントリーは抽出されません。@var{file}には、POファイルかPOTファイルを指定します。"

#. type: item
#: original_texis/xgettext.texi:124
#, no-wrap
msgid "-c[@var{tag}]"
msgstr "-c[@var{tag}]"

#. type: itemx
#: original_texis/xgettext.texi:125
#, no-wrap
msgid "--add-comments[=@var{tag}]"
msgstr "--add-comments[=@var{tag}]"

#. type: opindex
#: original_texis/xgettext.texi:126
#, no-wrap
msgid "-c@r{, @code{xgettext} option}"
msgstr "-c@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:127
#, no-wrap
msgid "--add-comments@r{, @code{xgettext} option}"
msgstr "--add-comments@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:131
msgid "Place comment blocks starting with @var{tag} and preceding keyword lines in the output file.  Without a @var{tag}, the option means to put @emph{all} comment blocks preceding keyword lines in the output file."
msgstr "@var{tag}で始まるコメントブロックを、出力ファイル中のキーワード行の前に配置します。このオプションで@var{tag}を指定しない場合には、出力ファイル中のすべてのキーワード行の前にコメントブロックが配置されます。"

#. type: table
#: original_texis/xgettext.texi:134
msgid "Note that comment blocks supposed to be extracted must be adjacent to keyword lines.  For example, in the following C source code:"
msgstr ""
"コメントブロックと想定されるブロックが抽出されるために、ブロックはキーワード行により調整されなければならないことに注意してください。\n"
"たとえば以下のCソースコードでは:"

#. type: example
#: original_texis/xgettext.texi:138
#, no-wrap
msgid ""
"/* This is the first comment.  */\n"
"gettext (\"foo\");\n"
"\n"
msgstr ""
"/* This is the first comment.  */\n"
"gettext (\"foo\");\n"
"\n"

#. type: example
#: original_texis/xgettext.texi:142
#, no-wrap
msgid ""
"/* This is the second comment: not extracted  */\n"
"gettext (\n"
"  \"bar\");\n"
"\n"
msgstr ""
"/* This is the second comment: not extracted  */\n"
"gettext (\n"
"  \"bar\");\n"
"\n"

#. type: example
#: original_texis/xgettext.texi:146
#, no-wrap
msgid ""
"gettext (\n"
"  /* This is the third comment.  */\n"
"  \"baz\");\n"
msgstr ""
"gettext (\n"
"  /* This is the third comment.  */\n"
"  \"baz\");\n"

#. type: table
#: original_texis/xgettext.texi:150
msgid "The second comment line will not be extracted, because there is one blank line between the comment line and the keyword."
msgstr "2番目のコメント行は抽出されないでしょう。なぜならコメント行とキーワードの間にブランク行が1つあるからです。"

#. type: item
#: original_texis/xgettext.texi:151
#, no-wrap
msgid "--check[=@var{CHECK}]"
msgstr "--check[=@var{CHECK}]"

#. type: opindex
#: original_texis/xgettext.texi:152
#, no-wrap
msgid "--check@r{, @code{xgettext} option}"
msgstr "--check@r{, @code{xgettext} option}"

#. type: cindex
#: original_texis/xgettext.texi:153
#, no-wrap
msgid "supported syntax checks, @code{xgettext}"
msgstr "supported syntax checks, @code{xgettext}"

#. type: table
#: original_texis/xgettext.texi:156
msgid "Perform a syntax check on msgid and msgid_plural.  The supported checks are:"
msgstr "msgidおよびmsgid_pluralにたいして構文チェックを行います。サポートされているチェックは:"

#. type: item
#: original_texis/xgettext.texi:158
#, no-wrap
msgid "ellipsis-unicode"
msgstr "ellipsis-unicode"

#. type: table
#: original_texis/xgettext.texi:160
msgid "Prefer Unicode ellipsis character over ASCII @code{...}"
msgstr "ASCIIの@code{...}より、Unicodeのellipsis文字を優先します。"

#. type: item
#: original_texis/xgettext.texi:161
#, no-wrap
msgid "space-ellipsis"
msgstr "space-ellipsis"

#. type: table
#: original_texis/xgettext.texi:163
msgid "Prohibit whitespace before an ellipsis character"
msgstr "ellipsis文字の前の空白文字を抑制します。"

#. type: item
#: original_texis/xgettext.texi:164
#, no-wrap
msgid "quote-unicode"
msgstr "quote-unicode"

#. type: table
#: original_texis/xgettext.texi:166
msgid "Prefer Unicode quotation marks over ASCII @code{\"'`}"
msgstr "ASCIIの@code{\"'`}より、Unicodeのクォーテーションマークを優先します。"

#. type: item
#: original_texis/xgettext.texi:167
#, no-wrap
msgid "bullet-unicode"
msgstr "bullet-unicode"

#. type: table
#: original_texis/xgettext.texi:169
msgid "Prefer Unicode bullet character over ASCII @code{*} or @code{-}"
msgstr "ASCIIの@code{*}または@code{-}より、Unicodeのbullet文字を優先します。"

#. type: table
#: original_texis/xgettext.texi:177
msgid "The option has an effect on all input files.  To enable or disable checks for a certain string, you can mark it with an @code{xgettext:} special comment in the source file.  For example, if you specify the @code{--check=space-ellipsis} option, but want to suppress the check on a particular string, add the following comment:"
msgstr "オプションはすべての入力ファイルに効果をもちます。特定の文字列にたいするチェックを有効、または無効にするために、ソースファイル中で特別なコメント@code{xgettext:}でマークすることができます。たとえば、@code{--check=space-ellipsis}を指定して、なおかつ特定の文字列にたいしてチェックを行いたくない場合には、以下のコメントを追加します:"

#. type: example
#: original_texis/xgettext.texi:181
#, no-wrap
msgid ""
"/* xgettext: no-space-ellipsis-check */\n"
"gettext (\"We really want a space before ellipsis here ...\");\n"
msgstr ""
"/* xgettext: no-space-ellipsis-check */\n"
"gettext (\"We really want a space before ellipsis here ...\");\n"

#. type: table
#: original_texis/xgettext.texi:187
msgid "The @code{xgettext:} comment can be followed by flags separated with a comma.  The possible flags are of the form @samp{[no-]@var{name}-check}, where @var{name} is the name of a valid syntax check.  If a flag is prefixed by @code{no-}, the meaning is negated."
msgstr "@code{xgettext:}コメントの後に、カンマで区切られたフラグを記述できます。利用可能なフラグは、@samp{[no-]@var{name}-check}という形式で、@var{name}は有効な構文チェックです。フラグのプレフィクスが@code{no-}の場合は、否定を意味します。"

#. type: table
#: original_texis/xgettext.texi:193
msgid "Some tests apply the checks to each sentence within the msgid, rather than the whole string.  xgettext detects the end of sentence by performing a pattern match, which usually looks for a period followed by a certain number of spaces.  The number is specified with the @code{--sentence-end} option."
msgstr "文字列全体ではなく、msgid内の各センテンスへのチェックに適用されるテストがいくつかあります。xgettextはパターンマッチを行うことによりセンテンスの終わりを検出し、通常は特定の個数のスペースを後にともなうピリオドを探します。この数は@code{--sentence-end}オプションで指定されます。"

#. type: item
#: original_texis/xgettext.texi:194
#, no-wrap
msgid "--sentence-end[=@var{TYPE}]"
msgstr "--sentence-end[=@var{TYPE}]"

#. type: opindex
#: original_texis/xgettext.texi:195
#, no-wrap
msgid "--sentence-end@r{, @code{xgettext} option}"
msgstr "--sentence-end@r{, @code{xgettext} option}"

#. type: cindex
#: original_texis/xgettext.texi:196
#, no-wrap
msgid "sentence end markers, @code{xgettext}"
msgstr "sentence end markers, @code{xgettext}"

#. type: table
#: original_texis/xgettext.texi:198
msgid "The supported values are:"
msgstr "サポートされる値は:"

#. type: item
#: original_texis/xgettext.texi:200
#, no-wrap
msgid "single-space"
msgstr "single-space"

#. type: table
#: original_texis/xgettext.texi:202
msgid "Expect at least one whitespace after a period"
msgstr "ピリオドの後に少なくとも1つの空白文字を要求します。"

#. type: item
#: original_texis/xgettext.texi:203
#, no-wrap
msgid "double-space"
msgstr "double-space"

#. type: table
#: original_texis/xgettext.texi:205
msgid "Expect at least two whitespaces after a period"
msgstr "ピリオドの後に少なくとも2つの空白文字を要求します。"

#. type: subsection
#: original_texis/xgettext.texi:210
#, no-wrap
msgid "Language specific options"
msgstr "Language specific options"

#. type: item
#: original_texis/xgettext.texi:213
#, no-wrap
msgid "-a"
msgstr "-a"

#. type: itemx
#: original_texis/xgettext.texi:214
#, no-wrap
msgid "--extract-all"
msgstr "--extract-all"

#. type: opindex
#: original_texis/xgettext.texi:215
#, no-wrap
msgid "-a@r{, @code{xgettext} option}"
msgstr "-a@r{, @code{xgettext} option}"

#. type: opindex
#: original_texis/xgettext.texi:216
#, no-wrap
msgid "--extract-all@r{, @code{xgettext} option}"
msgstr "--extract-all@r{, @code{xgettext} option}"

#. type: table
#: original_texis/xgettext.texi:218
msgid "Extract all strings."
msgstr "すべての文字列を抽出します。"

#. type: table
#: original_texis/xgettext.texi:222
