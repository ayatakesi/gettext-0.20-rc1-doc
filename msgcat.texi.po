#: original_texis/msgcat.texi:73
#, no-wrap
msgid "--less-than@r{, @code{msgcat} option}"
msgstr "--less-than@r{, @code{msgcat} option}"

#. type: table
#: original_texis/msgcat.texi:76
msgid "Print messages with less than @var{number} definitions, defaults to infinite if not set."
msgstr "@var{number}に指定した数より少ないメッセージを出力します。指定しなかった場合のデフォルトは無限大です。"

#. type: item
#: original_texis/msgcat.texi:77
#, no-wrap
msgid "-> @var{number}"
msgstr "-> @var{number}"

#. type: itemx
#: original_texis/msgcat.texi:78
#, no-wrap
msgid "--more-than=@var{number}"
msgstr "--more-than=@var{number}"

#. type: opindex
#: original_texis/msgcat.texi:79
#, no-wrap
msgid "->@r{, @code{msgcat} option}"
msgstr "->@r{, @code{msgcat} option}"

#. type: opindex
#: original_texis/msgcat.texi:80
#, no-wrap
msgid "--more-than@r{, @code{msgcat} option}"
msgstr "--more-than@r{, @code{msgcat} option}"

#. type: table
#: original_texis/msgcat.texi:83
msgid "Print messages with more than @var{number} definitions, defaults to 0 if not set."
msgstr "@var{number}に指定した数より大きいメッセージを出力します。指定しなかった場合のデフォルトは0です。"

#. type: item
#: original_texis/msgcat.texi:84
#, no-wrap
msgid "-u"
msgstr "-u"

#. type: itemx
#: original_texis/msgcat.texi:85
#, no-wrap
msgid "--unique"
msgstr "--unique"

#. type: opindex
#: original_texis/msgcat.texi:86
#, no-wrap
msgid "-u@r{, @code{msgcat} option}"
msgstr "-u@r{, @code{msgcat} option}"

#. type: opindex
#: original_texis/msgcat.texi:87
#, no-wrap
msgid "--unique@r{, @code{msgcat} option}"
msgstr "--unique@r{, @code{msgcat} option}"

#. type: table
#: original_texis/msgcat.texi:90
msgid "Shorthand for @samp{--less-than=2}.  Requests that only unique messages be printed."
msgstr "@samp{--less-than=2}の省略指定です。一意なメッセージだけを出力します。"

#. type: subsection
#: original_texis/msgcat.texi:93
#, no-wrap
