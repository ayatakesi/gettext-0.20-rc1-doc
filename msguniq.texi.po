#. type: opindex
#: original_texis/msguniq.texi:134
#, no-wrap
msgid "--add-location@r{, @code{msguniq} option}"
msgstr "--add-location@r{, @code{msguniq} option}"

#. type: table
#: original_texis/msguniq.texi:136
msgid "Generate @samp{#: @var{filename}:@var{line}} lines (default)."
msgstr "@samp{#: @var{filename}:@var{line}}という行を生成します(デフォルト)。"

#. type: table
#: original_texis/msguniq.texi:142
msgid "The optional @var{type} can be either @samp{full}, @samp{file}, or @samp{never}.  If it is not given or @samp{full}, it generates the lines with both file name and line number.  If it is @samp{file}, the line number part is omitted.  If it is @samp{never}, it completely suppresses the lines (same as @code{--no-location})."
msgstr "@var{type}はオプションで、@samp{full}、@samp{file}、または@samp{never}を指定できます。オプションが指定されない、または@samp{full}の場合は、ファイル名と行番号のの両方が生成されます。@samp{file}の場合、行番号は省略されます。@samp{never}の場合は、完全にこの行を抑制します(@code{--no-location}と同じです)。"

#. type: item
#: original_texis/msguniq.texi:143
#, no-wrap
msgid "--strict"
msgstr "--strict"

#. type: opindex
#: original_texis/msguniq.texi:144
#, no-wrap
msgid "--strict@r{, @code{msguniq} option}"
msgstr "--strict@r{, @code{msguniq} option}"

#. type: table
#: original_texis/msguniq.texi:148
msgid "Write out a strict Uniforum conforming PO file.  Note that this Uniforum format should be avoided because it doesn't support the GNU extensions."
msgstr "Uniforumに厳密に準拠したPOファイルを出力します。このUniforum形式はGNUの拡張をサポートしないため避けたほうがよいでしょう。"

#. type: item
#: original_texis/msguniq.texi:149
#, no-wrap
msgid "-p"
msgstr "-p"

#. type: itemx
#: original_texis/msguniq.texi:150
#, no-wrap
msgid "--properties-output"
msgstr "--properties-output"

#. type: opindex
#: original_texis/msguniq.texi:151
#, no-wrap
msgid "-p@r{, @code{msguniq} option}"
msgstr "-p@r{, @code{msguniq} option}"

#. type: opindex
#: original_texis/msguniq.texi:152
#, no-wrap
msgid "--properties-output@r{, @code{msguniq} option}"
msgstr "--properties-output@r{, @code{msguniq} option}"

#. type: table
#: original_texis/msguniq.texi:156
msgid "Write out a Java ResourceBundle in Java @code{.properties} syntax.  Note that this file format doesn't support plural forms and silently drops obsolete messages."
msgstr "Javaの@code{.properties}の書式で、Java ResourceBundleを出力します。このファイル形式はplural formをサポートせず、知能化したメッセージを暗黙で除去することに注意してください。"

#. type: item
#: original_texis/msguniq.texi:157
#, no-wrap
msgid "--stringtable-output"
msgstr "--stringtable-output"

#. type: opindex
#: original_texis/msguniq.texi:158
#, no-wrap
msgid "--stringtable-output@r{, @code{msguniq} option}"
msgstr "--stringtable-output@r{, @code{msguniq} option}"

#. type: table
#: original_texis/msguniq.texi:161
msgid "Write out a NeXTstep/GNUstep localized resource file in @code{.strings} syntax.  Note that this file format doesn't support plural forms."
msgstr "@code{.strings}の書式で、NeXTstep/GNUstepのローカライズされたリソースファイルを出力します。このファイル形式はplural formをサポートしないことに注意してください。"

#. type: item
#: original_texis/msguniq.texi:162
#, no-wrap
msgid "-w @var{number}"
msgstr "-w @var{number}"

#. type: itemx
#: original_texis/msguniq.texi:163
#, no-wrap
msgid "--width=@var{number}"
msgstr "--width=@var{number}"

#. type: opindex
#: original_texis/msguniq.texi:164
#, no-wrap
