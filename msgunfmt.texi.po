#. type: pindex
#: original_texis/msgunfmt.texi:5
#, no-wrap
msgid "msgunfmt"
msgstr "msgunfmt"

#. type: cindex
#: original_texis/msgunfmt.texi:6
#, no-wrap
msgid "@code{msgunfmt} program, usage"
msgstr "@code{msgunfmt} program, usage"

#. type: example
#: original_texis/msgunfmt.texi:9
#, no-wrap
msgid "msgunfmt [@var{option}] [@var{file}]...\n"
msgstr "msgunfmt [@var{option}] [@var{file}]...\n"

#. type: cindex
#: original_texis/msgunfmt.texi:11
#, no-wrap
msgid "convert binary message catalog into PO file"
msgstr "convert binary message catalog into PO file"

#. type: Plain text
#: original_texis/msgunfmt.texi:14
msgid "The @code{msgunfmt} program converts a binary message catalog to a Uniforum style .po file."
msgstr "@code{msgunfmt}は、バイナリーのメッセージカタログを、Uniforum形式の.poファイルに変換するプログラムです。"

