msgstr "Tclモード: tcl/msgcatの@file{.msg}ファイルが入力となります。"

#. type: subsection
#: original_texis/msgunfmt.texi:43
#, no-wrap
msgid "Input file location"
msgstr "Input file location"

#. type: item
#: original_texis/msgunfmt.texi:46
#, no-wrap
msgid "@var{file} @dots{}"
msgstr "@var{file} @dots{}"

#. type: table
#: original_texis/msgunfmt.texi:48
msgid "Input .mo files."
msgstr "入力となる.moファイルです。"

#. type: Plain text
#: original_texis/msgunfmt.texi:52
msgid "If no input @var{file} is given or if it is @samp{-}, standard input is read."
msgstr "@var{file}が指定されていないか、@samp{-}が指定された場合は、標準入力から読み込みます。"

#. type: subsection
#: original_texis/msgunfmt.texi:53
#, no-wrap
msgid "Input file location in Java mode"
msgstr "Input file location in Java mode"

#. type: item
#: original_texis/msgunfmt.texi:56 original_texis/msgunfmt.texi:77
#, no-wrap
msgid "-r @var{resource}"
msgstr "-r @var{resource}"

#. type: itemx
#: original_texis/msgunfmt.texi:57 original_texis/msgunfmt.texi:78
#, no-wrap
