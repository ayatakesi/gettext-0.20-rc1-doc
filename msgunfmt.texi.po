#. type: opindex
#: original_texis/msgunfmt.texi:59 original_texis/msgunfmt.texi:80
#, no-wrap
msgid "--resource@r{, @code{msgunfmt} option}"
msgstr "--resource@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:61 original_texis/msgunfmt.texi:82
msgid "Specify the resource name."
msgstr "リソース名を指定します。"

#. type: item
#: original_texis/msgunfmt.texi:62 original_texis/msgunfmt.texi:83
#: original_texis/msgunfmt.texi:103
#, no-wrap
msgid "-l @var{locale}"
msgstr "-l @var{locale}"

#. type: itemx
#: original_texis/msgunfmt.texi:63 original_texis/msgunfmt.texi:84
#: original_texis/msgunfmt.texi:104
#, no-wrap
msgid "--locale=@var{locale}"
msgstr "--locale=@var{locale}"

#. type: opindex
#: original_texis/msgunfmt.texi:64 original_texis/msgunfmt.texi:85
#: original_texis/msgunfmt.texi:105
#, no-wrap
msgid "-l@r{, @code{msgunfmt} option}"
msgstr "-l@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:65 original_texis/msgunfmt.texi:86
#: original_texis/msgunfmt.texi:106
#, no-wrap
msgid "--locale@r{, @code{msgunfmt} option}"
msgstr "--locale@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:68 original_texis/msgunfmt.texi:89
#: original_texis/msgunfmt.texi:109
msgid "Specify the locale name, either a language specification of the form @var{ll} or a combined language and country specification of the form @var{ll_CC}."
msgstr "locale名を指定します。@var{ll}形式による言語指定と、国と言語指定を組み合わせた@var{ll_CC}のどちらでも指定できます。"

#. type: Plain text
#: original_texis/msgunfmt.texi:73
msgid "The class name is determined by appending the locale name to the resource name, separated with an underscore.  The class is located using the @code{CLASSPATH}."
msgstr "class名は、resource名の後ろにアンダースコアーをつけて、その後ろにlocale 名を付加することにより決定されます。classは、@code{CLASSPATH}によって配置されます。"

#. type: subsection
#: original_texis/msgunfmt.texi:74
#, no-wrap
msgid "Input file location in C# mode"
msgstr "Input file location in C# mode"

#. type: item
#: original_texis/msgunfmt.texi:90 original_texis/msgunfmt.texi:110
#, no-wrap
msgid "-d @var{directory}"
msgstr "-d @var{directory}"

#. type: opindex
#: original_texis/msgunfmt.texi:91 original_texis/msgunfmt.texi:111
#, no-wrap
msgid "-d@r{, @code{msgunfmt} option}"
msgstr "-d@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:93
msgid "Specify the base directory for locale dependent @file{.dll} files."
msgstr "localeに依存する@file{.dll}ファイルを出力するベースディレクトリーを指定します。"

#. type: Plain text
#: original_texis/msgunfmt.texi:99
msgid "The @samp{-l} and @samp{-d} options are mandatory.  The @file{.msg} file is located in a subdirectory of the specified directory whose name depends on the locale."
msgstr "@samp{-l}と@samp{-d}が必須オプションです。@file{.msg}ファイルは、locale に依存した名前の指定ディレクトリーのサブディレクトリーに配置されています。"

#. type: subsection
#: original_texis/msgunfmt.texi:100
#, no-wrap
msgid "Input file location in Tcl mode"
msgstr "Input file location in Tcl mode"

#. type: table
#: original_texis/msgunfmt.texi:113
msgid "Specify the base directory of @file{.msg} message catalogs."
msgstr "メッセージカタログ@file{.msg}のベースディレクトリーを指定します。"

#. type: Plain text
#: original_texis/msgunfmt.texi:118
msgid "The @samp{-l} and @samp{-d} options are mandatory.  The @file{.msg} file is located in the specified directory."
msgstr "@samp{-l}と@samp{-d}は必須オプションです。@file{.msg}は指定されたディレクトリーに配置されています。"

#. type: subsection
#: original_texis/msgunfmt.texi:119
#, no-wrap
msgid "Output file location"
msgstr "Output file location"

#. type: item
#: original_texis/msgunfmt.texi:122
#, no-wrap
msgid "-o @var{file}"
msgstr "-o @var{file}"

#. type: itemx
#: original_texis/msgunfmt.texi:123
#, no-wrap
msgid "--output-file=@var{file}"
msgstr "--output-file=@var{file}"

#. type: opindex
#: original_texis/msgunfmt.texi:124
#, no-wrap
msgid "-o@r{, @code{msgunfmt} option}"
msgstr "-o@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:125
#, no-wrap
msgid "--output-file@r{, @code{msgunfmt} option}"
msgstr "--output-file@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:127
msgid "Write output to specified file."
msgstr "指定されたファイルに出力を書き込みます。"

#. type: Plain text
#: original_texis/msgunfmt.texi:132
msgid "The results are written to standard output if no output file is specified or if it is @samp{-}."
msgstr "出力ファイルが指定されていない、または@samp{-}が指定された場合、結果は標準出力に出力されます。"

#. type: subsection
#: original_texis/msgunfmt.texi:133
#, no-wrap
msgid "Output details"
msgstr "Output details"

#. type: item
#: original_texis/msgunfmt.texi:138
#, no-wrap
msgid "--color"
msgstr "--color"

#. type: itemx
#: original_texis/msgunfmt.texi:139
#, no-wrap
msgid "--color=@var{when}"
msgstr "--color=@var{when}"

#. type: opindex
#: original_texis/msgunfmt.texi:140
#, no-wrap
msgid "--color@r{, @code{msgunfmt} option}"
msgstr "--color@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:143
msgid "Specify whether or when to use colors and other text attributes.  See @ref{The --color option} for details."
msgstr "色や色以外のテキスト属性を使うか、いつ使うかを指定します。詳細は@ref{The --color option}を参照してください。"

#. type: item
#: original_texis/msgunfmt.texi:144
#, no-wrap
msgid "--style=@var{style_file}"
msgstr "--style=@var{style_file}"

#. type: opindex
#: original_texis/msgunfmt.texi:145
#, no-wrap
msgid "--style@r{, @code{msgunfmt} option}"
msgstr "--style@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:148
msgid "Specify the CSS style rule file to use for @code{--color}.  See @ref{The --style option} for details."
msgstr "@code{--color}にたいしてCSS style ruleファイルを使うかを指定します。詳細は@ref{The --style option}を参照してください。"

#. type: item
#: original_texis/msgunfmt.texi:149
#, no-wrap
msgid "--force-po"
msgstr "--force-po"

#. type: opindex
#: original_texis/msgunfmt.texi:150
#, no-wrap
msgid "--force-po@r{, @code{msgunfmt} option}"
msgstr "--force-po@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:152
msgid "Always write an output file even if it contains no message."
msgstr "メッセージが何も含まれていない場合でも、常に出力ファイルに書き込みます。"

#. type: item
#: original_texis/msgunfmt.texi:153
#, no-wrap
msgid "-i"
msgstr "-i"

#. type: itemx
#: original_texis/msgunfmt.texi:154
#, no-wrap
msgid "--indent"
msgstr "--indent"

#. type: opindex
#: original_texis/msgunfmt.texi:155
#, no-wrap
msgid "-i@r{, @code{msgunfmt} option}"
msgstr "-i@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:156
#, no-wrap
msgid "--indent@r{, @code{msgunfmt} option}"
msgstr "--indent@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:158
msgid "Write the .po file using indented style."
msgstr "インデントされた形式で.poファイルを書き込みます。"

#. type: item
#: original_texis/msgunfmt.texi:159
#, no-wrap
msgid "--strict"
msgstr "--strict"

#. type: opindex
#: original_texis/msgunfmt.texi:160
#, no-wrap
msgid "--strict@r{, @code{msgunfmt} option}"
msgstr "--strict@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:164
msgid "Write out a strict Uniforum conforming PO file.  Note that this Uniforum format should be avoided because it doesn't support the GNU extensions."
msgstr "Uniforumに厳密に準拠したPOファイルを出力します。このUniforum形式はGNUの拡張をサポートしないため避けたほうがよいでしょう。"

#. type: item
#: original_texis/msgunfmt.texi:165
#, no-wrap
msgid "-p"
msgstr "-p"

#. type: itemx
#: original_texis/msgunfmt.texi:166
#, no-wrap
msgid "--properties-output"
msgstr "--properties-output"

#. type: opindex
#: original_texis/msgunfmt.texi:167
#, no-wrap
msgid "-p@r{, @code{msgunfmt} option}"
msgstr "-p@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:168
#, no-wrap
msgid "--properties-output@r{, @code{msgunfmt} option}"
msgstr "--properties-output@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:172
msgid "Write out a Java ResourceBundle in Java @code{.properties} syntax.  Note that this file format doesn't support plural forms and silently drops obsolete messages."
msgstr "Javaの@code{.properties}の書式で、Java ResourceBundleを出力します。このファイル形式はplural formをサポートせず、陳腐化したメッセージを暗黙で除去することに注意してください。"

#. type: item
#: original_texis/msgunfmt.texi:173
#, no-wrap
msgid "--stringtable-output"
msgstr "--stringtable-output"

#. type: opindex
#: original_texis/msgunfmt.texi:174
#, no-wrap
msgid "--stringtable-output@r{, @code{msgunfmt} option}"
msgstr "--stringtable-output@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:177
msgid "Write out a NeXTstep/GNUstep localized resource file in @code{.strings} syntax.  Note that this file format doesn't support plural forms."
msgstr "@code{.strings}の書式で、NeXTstep/GNUstepのローカライズされたリソースファイルを出力します。このファイル形式はplural formをサポートしないことに注意してください。"

#. type: item
#: original_texis/msgunfmt.texi:178
#, no-wrap
msgid "-w @var{number}"
msgstr "-w @var{number}"

#. type: itemx
#: original_texis/msgunfmt.texi:179
#, no-wrap
msgid "--width=@var{number}"
msgstr "--width=@var{number}"

#. type: opindex
#: original_texis/msgunfmt.texi:180
#, no-wrap
msgid "-w@r{, @code{msgunfmt} option}"
msgstr "-w@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:181
#, no-wrap
msgid "--width@r{, @code{msgunfmt} option}"
msgstr "--width@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:185
msgid "Set the output page width.  Long strings in the output files will be split across multiple lines in order to ensure that each line's width (= number of screen columns) is less or equal to the given @var{number}."
msgstr "出力ページの幅をセットします。これにより出力ファイル中の長い文字列が指定した幅(例:スクリーンの列数)に収まるように、各行の長さが@var{number}以下のような複数の行に分割されます。"

#. type: item
#: original_texis/msgunfmt.texi:186
#, no-wrap
msgid "--no-wrap"
msgstr "--no-wrap"

#. type: opindex
#: original_texis/msgunfmt.texi:187
#, no-wrap
msgid "--no-wrap@r{, @code{msgunfmt} option}"
msgstr "--no-wrap@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:191
msgid "Do not break long message lines.  Message lines whose width exceeds the output page width will not be split into several lines.  Only file reference lines which are wider than the output page width will be split."
msgstr "長いメッセージ行を分割しません。出力ページの幅を超えるようなメッセージ行も、複数行に分割されません。出力ページの幅を超えるファイル参照行だけが分割されます。"

#. type: item
#: original_texis/msgunfmt.texi:192
#, no-wrap
msgid "-s"
msgstr "-s"

#. type: itemx
#: original_texis/msgunfmt.texi:193
#, no-wrap
msgid "--sort-output"
msgstr "--sort-output"

#. type: opindex
#: original_texis/msgunfmt.texi:194
#, no-wrap
msgid "-s@r{, @code{msgunfmt} option}"
msgstr "-s@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:195
#, no-wrap
msgid "--sort-output@r{, @code{msgunfmt} option}"
msgstr "--sort-output@r{, @code{msgunfmt} option}"

#. type: cindex
#: original_texis/msgunfmt.texi:196
#, no-wrap
msgid "sorting @code{msgunfmt} output"
msgstr "sorting @code{msgunfmt} output"

#. type: table
#: original_texis/msgunfmt.texi:199
msgid "Generate sorted output.  Note that using this option makes it much harder for the translator to understand each message's context."
msgstr "ソートされた出力を生成します。このオプションを使用することにより翻訳者が、メッセージがどのようなコンテキストで使用されるかを理解するのが、困難になることに注意してください。"

#. type: subsection
#: original_texis/msgunfmt.texi:202
#, no-wrap
msgid "Informative output"
msgstr "Informative output"

#. type: item
#: original_texis/msgunfmt.texi:205
#, no-wrap
msgid "-h"
msgstr "-h"

#. type: itemx
#: original_texis/msgunfmt.texi:206
#, no-wrap
msgid "--help"
msgstr "--help"

#. type: opindex
#: original_texis/msgunfmt.texi:207
#, no-wrap
msgid "-h@r{, @code{msgunfmt} option}"
msgstr "-h@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:208
#, no-wrap
msgid "--help@r{, @code{msgunfmt} option}"
msgstr "--help@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:210
msgid "Display this help and exit."
msgstr "このヘルプを表示して終了します。"

#. type: item
#: original_texis/msgunfmt.texi:211
#, no-wrap
msgid "-V"
msgstr "-V"

#. type: itemx
#: original_texis/msgunfmt.texi:212
#, no-wrap
msgid "--version"
msgstr "--version"

#. type: opindex
#: original_texis/msgunfmt.texi:213
#, no-wrap
msgid "-V@r{, @code{msgunfmt} option}"
msgstr "-V@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:214
#, no-wrap
msgid "--version@r{, @code{msgunfmt} option}"
msgstr "--version@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:216
msgid "Output version information and exit."
msgstr "バージョン情報を表示して終了します。"

#. type: item
#: original_texis/msgunfmt.texi:217
#, no-wrap
msgid "-v"
msgstr "-v"

#. type: itemx
#: original_texis/msgunfmt.texi:218
#, no-wrap
msgid "--verbose"
msgstr "--verbose"

#. type: opindex
#: original_texis/msgunfmt.texi:219
#, no-wrap
msgid "-v@r{, @code{msgunfmt} option}"
msgstr "-v@r{, @code{msgunfmt} option}"

#. type: opindex
#: original_texis/msgunfmt.texi:220
#, no-wrap
msgid "--verbose@r{, @code{msgunfmt} option}"
msgstr "--verbose@r{, @code{msgunfmt} option}"

#. type: table
#: original_texis/msgunfmt.texi:222
msgid "Increase verbosity level."
msgstr "診断レベルを上げます。"
