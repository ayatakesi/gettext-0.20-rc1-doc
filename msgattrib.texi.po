#. type: pindex
#: original_texis/msgattrib.texi:5
#, no-wrap
msgid "msgattrib"
msgstr "msgattrib"

#. type: cindex
#: original_texis/msgattrib.texi:6
#, no-wrap
msgid "@code{msgattrib} program, usage"
msgstr "@code{msgattrib} program, usage"

#. type: example
#: original_texis/msgattrib.texi:9
#, no-wrap
msgid "msgattrib [@var{option}] [@var{inputfile}]\n"
msgstr "msgattrib [@var{option}] [@var{inputfile}]\n"

#. type: cindex
#: original_texis/msgattrib.texi:11
#, no-wrap
msgid "filter messages according to attributes"
msgstr "filter messages according to attributes"

#. type: cindex
#: original_texis/msgattrib.texi:12
#, no-wrap
msgid "attribute manipulation"
msgstr "attribute manipulation"

#. type: Plain text
#: original_texis/msgattrib.texi:15
msgid "The @code{msgattrib} program filters the messages of a translation catalog according to their attributes, and manipulates the attributes."
msgstr "@code{msgattrib}プログラムは、翻訳カタログのメッセージの属性にしたがってフィルターを適用したり、属性を操作するためのプログラムです。"

#. type: subsection
#: original_texis/msgattrib.texi:16
#, no-wrap
msgid "Input file location"
msgstr "Input file location"

#. type: var{#1}
#: original_texis/msgattrib.texi:19
#, no-wrap
msgid "inputfile"
msgstr "inputfile"

#. type: table
#: original_texis/msgattrib.texi:21
msgid "Input PO file."
msgstr "入力となるPOファイルです。"

#. type: item
#: original_texis/msgattrib.texi:22
#, no-wrap
msgid "-D @var{directory}"
msgstr "-D @var{directory}"

#. type: itemx
#: original_texis/msgattrib.texi:23
#, no-wrap
msgid "--directory=@var{directory}"
msgstr "--directory=@var{directory}"

#. type: opindex
#: original_texis/msgattrib.texi:24
#, no-wrap
msgid "-D@r{, @code{msgattrib} option}"
msgstr "-D@r{, @code{msgattrib} option}"

#. type: opindex
#: original_texis/msgattrib.texi:25
#, no-wrap
msgid "--directory@r{, @code{msgattrib} option}"
msgstr "--directory@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:29
msgid "Add @var{directory} to the list of directories.  Source files are searched relative to this list of directories.  The resulting @file{.po} file will be written relative to the current directory, though."
msgstr "ディレクトリーのリストに@var{directory}を追加します。このディレクトリーのリストよりソースファイルを検索します。しかし@file{.po}ファイルが出力されるのは、カレントディレクトリーです。"

#. type: Plain text
#: original_texis/msgattrib.texi:33
msgid "If no @var{inputfile} is given or if it is @samp{-}, standard input is read."
msgstr "@var{inputfile}が指定されていないか、@samp{-}が指定された場合は、標準入力から読み込みます。"

#. type: subsection
#: original_texis/msgattrib.texi:34
#, no-wrap
msgid "Output file location"
msgstr "Output file location"

#. type: item
#: original_texis/msgattrib.texi:37
#, no-wrap
msgid "-o @var{file}"
msgstr "-o @var{file}"

#. type: itemx
#: original_texis/msgattrib.texi:38
#, no-wrap
msgid "--output-file=@var{file}"
msgstr "--output-file=@var{file}"

#. type: opindex
#: original_texis/msgattrib.texi:39
#, no-wrap
msgid "-o@r{, @code{msgattrib} option}"
msgstr "-o@r{, @code{msgattrib} option}"

#. type: opindex
#: original_texis/msgattrib.texi:40
#, no-wrap
msgid "--output-file@r{, @code{msgattrib} option}"
msgstr "--output-file@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:42
msgid "Write output to specified file."
msgstr "指定されたファイルに出力を書き込みます。"

#. type: Plain text
#: original_texis/msgattrib.texi:47
msgid "The results are written to standard output if no output file is specified or if it is @samp{-}."
msgstr "出力ファイルが指定されていない、または@samp{-}が指定された場合、結果は標準出力に出力されます。"

#. type: subsection
#: original_texis/msgattrib.texi:48
#, no-wrap
msgid "Message selection"
msgstr "Message selection"

#. type: item
