#: original_texis/msgattrib.texi:134 original_texis/msgattrib.texi:149
#: original_texis/msgattrib.texi:171
msgid "‘fuzzy’"
msgstr "‘fuzzy’"

#. type: ifnothtml
#: original_texis/msgattrib.texi:67 original_texis/msgattrib.texi:78
#: original_texis/msgattrib.texi:137 original_texis/msgattrib.texi:152
#: original_texis/msgattrib.texi:174
msgid "`fuzzy'"
msgstr "`fuzzy'"

#. type: table
#: original_texis/msgattrib.texi:69
msgid "marked messages."
msgstr "とマークされたメッセージを削除します。"

#. type: item
#: original_texis/msgattrib.texi:70
#, no-wrap
msgid "--only-fuzzy"
msgstr "--only-fuzzy"

#. type: opindex
#: original_texis/msgattrib.texi:71
#, no-wrap
msgid "--only-fuzzy@r{, @code{msgattrib} option}"
msgstr "--only-fuzzy@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:73
msgid "Keep"
msgstr "保持: "

#. type: table
#: original_texis/msgattrib.texi:80
msgid "marked messages, remove all other messages."
msgstr "とマークされたメッセージを残して、他のすべてのメッセージは削除します。"

#. type: item
#: original_texis/msgattrib.texi:81
#, no-wrap
msgid "--no-obsolete"
msgstr "--no-obsolete"

#. type: opindex
#: original_texis/msgattrib.texi:82
#, no-wrap
msgid "--no-obsolete@r{, @code{msgattrib} option}"
msgstr "--no-obsolete@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:84
msgid "Remove obsolete #~ messages."
msgstr "#~のついた廃止されたメッセージを削除します。"

#. type: item
#: original_texis/msgattrib.texi:85
#, no-wrap
msgid "--only-obsolete"
msgstr "--only-obsolete"

#. type: opindex
#: original_texis/msgattrib.texi:86
#, no-wrap
msgid "--only-obsolete@r{, @code{msgattrib} option}"
msgstr "--only-obsolete@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:88
msgid "Keep obsolete #~ messages, remove all other messages."
msgstr "#~のついた廃止されたメッセージを残して、他のすべてのメッセージは削除します。"

#. type: subsection
#: original_texis/msgattrib.texi:91
#, no-wrap
msgid "Attribute manipulation"
msgstr "Attribute manipulation"

#. type: cindex
#: original_texis/msgattrib.texi:93
#, no-wrap
msgid "modify message attributes"
msgstr "メッセージ属性修正"

#. type: Plain text
#: original_texis/msgattrib.texi:99
msgid "Attributes are modified after the message selection/removal has been performed.  If the @samp{--only-file} or @samp{--ignore-file} option is specified, the attribute modification is applied only to those messages that are listed in the @var{only-file} and not listed in the @var{ignore-file}."
msgstr "メッセージの選択・削除が実行された後に、属性は変更されます。@samp{--only-file}か@samp{--ignore-file}オプションを指定すると、@var{only-file}に記載されたメッセージ、もしくは@var{ignore-file}に記載されていないメッセージにだけ、変更が適用されます。"

#. type: item
#: original_texis/msgattrib.texi:101
#, no-wrap
msgid "--set-fuzzy"
msgstr "--set-fuzzy"

#. type: opindex
#: original_texis/msgattrib.texi:102
#, no-wrap
msgid "--set-fuzzy@r{, @code{msgattrib} option}"
msgstr "--set-fuzzy@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:104 original_texis/msgattrib.texi:114
msgid "Set all messages"
msgstr "すべてのメッセージに、"

#. type: ifhtml
#: original_texis/msgattrib.texi:106
msgid "‘fuzzy’."
msgstr "‘fuzzy’をセットします。"

#. type: ifnothtml
#: original_texis/msgattrib.texi:109
msgid "`fuzzy'."
msgstr "`fuzzy'をセットします。"

#. type: item
#: original_texis/msgattrib.texi:111
#, no-wrap
msgid "--clear-fuzzy"
msgstr "--clear-fuzzy"

#. type: opindex
#: original_texis/msgattrib.texi:112
#, no-wrap
msgid "--clear-fuzzy@r{, @code{msgattrib} option}"
msgstr "--clear-fuzzy@r{, @code{msgattrib} オプション}"

#. type: ifhtml
#: original_texis/msgattrib.texi:116
msgid "non-‘fuzzy’."
msgstr "非‘fuzzy’をセットします。"

#. type: ifnothtml
#: original_texis/msgattrib.texi:119
msgid "non-`fuzzy'."
msgstr "非`fuzzy'をセットします。"

#. type: item
#: original_texis/msgattrib.texi:121
#, no-wrap
msgid "--set-obsolete"
msgstr "--set-obsolete"

#. type: opindex
#: original_texis/msgattrib.texi:122
#, no-wrap
msgid "--set-obsolete@r{, @code{msgattrib} option}"
msgstr "--set-obsolete@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:124
msgid "Set all messages obsolete."
msgstr "すべてのメッセージを、陳腐化したメッセージにします。"

#. type: item
#: original_texis/msgattrib.texi:125
#, no-wrap
msgid "--clear-obsolete"
msgstr "--clear-obsolete"

#. type: opindex
#: original_texis/msgattrib.texi:126
#, no-wrap
msgid "--clear-obsolete@r{, @code{msgattrib} option}"
msgstr "--clear-obsolete@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:128
msgid "Set all messages non-obsolete."
msgstr "陳腐化したメッセージを、陳腐化していないメッセージにセットします。"

#. type: item
#: original_texis/msgattrib.texi:129
#, no-wrap
msgid "--previous"
msgstr "--previous"

#. type: opindex
#: original_texis/msgattrib.texi:130
#, no-wrap
msgid "--previous@r{, @code{msgattrib} option}"
msgstr "--previous@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:132
msgid "When setting"
msgstr "もし"

#. type: table
#: original_texis/msgattrib.texi:139
msgid "mark, keep ``previous msgid'' of translated messages."
msgstr "とマークされているとき、翻訳されたメッセージの``以前のmsgid''を残します。"

#. type: item
#: original_texis/msgattrib.texi:140
#, no-wrap
msgid "--clear-previous"
msgstr "--clear-previous"

#. type: opindex
#: original_texis/msgattrib.texi:141
#, no-wrap
msgid "--clear-previous@r{, @code{msgattrib} option}"
msgstr "--clear-previous@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:143
msgid "Remove the ``previous msgid'' (@samp{#|}) comments from all messages."
msgstr "すべてのメッセージから、``以前のmsgid''であることを示すコメントマーク(@samp{#|})がついたものを削除します。"

#. type: item
#: original_texis/msgattrib.texi:144
#, no-wrap
msgid "--empty"
msgstr "--empty"

#. type: opindex
#: original_texis/msgattrib.texi:145
#, no-wrap
msgid "--empty@r{, @code{msgattrib} option}"
msgstr "--empty@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:147
msgid "When removing"
msgstr "もし"

#. type: table
#: original_texis/msgattrib.texi:154
msgid "mark, also set msgstr empty."
msgstr "をマークした場合、msgstrも空にセットします。"

#. type: item
#: original_texis/msgattrib.texi:155
#, no-wrap
msgid "--only-file=@var{file}"
msgstr "--only-file=@var{file}"

#. type: opindex
#: original_texis/msgattrib.texi:156
#, no-wrap
msgid "--only-file@r{, @code{msgattrib} option}"
msgstr "--only-file@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:159
msgid "Limit the attribute changes to entries that are listed in @var{file}.  @var{file} should be a PO or POT file."
msgstr "@var{file}に記載されたエントリーの属性だけを変更します。@var{file}にはPO、またはPOTファイルを指定します。"

#. type: item
#: original_texis/msgattrib.texi:160
#, no-wrap
msgid "--ignore-file=@var{file}"
msgstr "--ignore-file=@var{file}"

#. type: opindex
#: original_texis/msgattrib.texi:161
#, no-wrap
msgid "--ignore-file@r{, @code{msgattrib} option}"
msgstr "--ignore-file@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:164
msgid "Limit the attribute changes to entries that are not listed in @var{file}.  @var{file} should be a PO or POT file."
msgstr "@var{file}に記載されていないエントリーの属性だけを変更します。@var{file}にはPO、またはPOTファイルを指定します。"

#. type: item
#: original_texis/msgattrib.texi:165
#, no-wrap
msgid "--fuzzy"
msgstr "--fuzzy"

#. type: opindex
#: original_texis/msgattrib.texi:166
#, no-wrap
msgid "--fuzzy@r{, @code{msgattrib} option}"
msgstr "--fuzzy@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:169
msgid "Synonym for @samp{--only-fuzzy --clear-fuzzy}: It keeps only the fuzzy messages and removes their"
msgstr "@samp{--only-fuzzy --clear-fuzzy}の省略指定です。fuzzyメッセージだけを残すとともに、それらのメッセージの"

#. type: table
#: original_texis/msgattrib.texi:176
msgid "mark."
msgstr "マークを外します。"

#. type: item
#: original_texis/msgattrib.texi:177
#, no-wrap
msgid "--obsolete"
msgstr "--obsolete"

#. type: opindex
#: original_texis/msgattrib.texi:178
#, no-wrap
msgid "--obsolete@r{, @code{msgattrib} option}"
msgstr "--obsolete@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:181
msgid "Synonym for @samp{--only-obsolete --clear-obsolete}: It keeps only the obsolete messages and makes them non-obsolete."
msgstr "@samp{--only-obsolete --clear-obsolete}の省略指定です。廃止されたメッセージだけを残すとともに、それらのメッセージが陳腐化していることを示すマークを外します。"

#. type: subsection
#: original_texis/msgattrib.texi:184
#, no-wrap
msgid "Input file syntax"
msgstr "Input file syntax"

#. type: item
#: original_texis/msgattrib.texi:187
#, no-wrap
msgid "-P"
msgstr "-P"

#. type: itemx
#: original_texis/msgattrib.texi:188
#, no-wrap
msgid "--properties-input"
msgstr "--properties-input"

#. type: opindex
#: original_texis/msgattrib.texi:189
#, no-wrap
msgid "-P@r{, @code{msgattrib} option}"
msgstr "-P@r{, @code{msgattrib} option}"

#. type: opindex
#: original_texis/msgattrib.texi:190
#, no-wrap
msgid "--properties-input@r{, @code{msgattrib} option}"
msgstr "--properties-input@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:193
msgid "Assume the input file is a Java ResourceBundle in Java @code{.properties} syntax, not in PO file syntax."
msgstr "入力ファイルがPOファイルの構文ではなく、Javaの@code{.properties}の構文にのっとったJava ResourceBundleファイルだとみなします。"

#. type: item
#: original_texis/msgattrib.texi:194
#, no-wrap
msgid "--stringtable-input"
msgstr "--stringtable-input"

#. type: opindex
#: original_texis/msgattrib.texi:195
#, no-wrap
msgid "--stringtable-input@r{, @code{msgattrib} option}"
msgstr "--stringtable-input@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:198
msgid "Assume the input file is a NeXTstep/GNUstep localized resource file in @code{.strings} syntax, not in PO file syntax."
msgstr "入力ファイルがPOファイルの構文ではなく、NeXTstep/GNUstepのlocalized resourceの@code{.strings}の構文にのっとったファイルだとみなします。"

#. type: subsection
#: original_texis/msgattrib.texi:201
#, no-wrap
msgid "Output details"
msgstr "Output details"

#. type: item
#: original_texis/msgattrib.texi:206
#, no-wrap
msgid "--color"
msgstr "--color"

#. type: itemx
#: original_texis/msgattrib.texi:207
#, no-wrap
msgid "--color=@var{when}"
msgstr "--color=@var{when}"

#. type: opindex
#: original_texis/msgattrib.texi:208
#, no-wrap
msgid "--color@r{, @code{msgattrib} option}"
msgstr "--color@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:211
msgid "Specify whether or when to use colors and other text attributes.  See @ref{The --color option} for details."
msgstr "色や色以外のテキスト属性を使うか、いつ使うかを指定します。詳細は@ref{The --color option}を参照してください。"

#. type: item
#: original_texis/msgattrib.texi:212
#, no-wrap
msgid "--style=@var{style_file}"
msgstr "--style=@var{style_file}"

#. type: opindex
#: original_texis/msgattrib.texi:213
#, no-wrap
msgid "--style@r{, @code{msgattrib} option}"
msgstr "--style@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:216
msgid "Specify the CSS style rule file to use for @code{--color}.  See @ref{The --style option} for details."
msgstr "@code{--color}にたいしてCSS style ruleファイルを使うかを指定します。詳細は@ref{The --style option}を参照してください。"

#. type: item
#: original_texis/msgattrib.texi:217
#, no-wrap
msgid "--force-po"
msgstr "--force-po"

#. type: opindex
#: original_texis/msgattrib.texi:218
#, no-wrap
msgid "--force-po@r{, @code{msgattrib} option}"
msgstr "--force-po@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:220
msgid "Always write an output file even if it contains no message."
msgstr "メッセージが何も含まれていない場合でも、常に出力ファイルに書き込みます。"

#. type: item
#: original_texis/msgattrib.texi:221
#, no-wrap
msgid "-i"
msgstr "-i"

#. type: itemx
#: original_texis/msgattrib.texi:222
#, no-wrap
msgid "--indent"
msgstr "--indent"

#. type: opindex
#: original_texis/msgattrib.texi:223
#, no-wrap
msgid "-i@r{, @code{msgattrib} option}"
msgstr "-i@r{, @code{msgattrib} option}"

#. type: opindex
#: original_texis/msgattrib.texi:224
#, no-wrap
msgid "--indent@r{, @code{msgattrib} option}"
msgstr "--indent@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:226
msgid "Write the .po file using indented style."
msgstr "インデントされた形式で.poファイルを書き込みます。"

#. type: item
#: original_texis/msgattrib.texi:227
#, no-wrap
msgid "--no-location"
msgstr "--no-location"

#. type: opindex
#: original_texis/msgattrib.texi:228
#, no-wrap
msgid "--no-location@r{, @code{msgattrib} option}"
msgstr "--no-location@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:230
msgid "Do not write @samp{#: @var{filename}:@var{line}} lines."
msgstr "@samp{#: @var{filename}:@var{line}}という行を書き込みません。"

#. type: item
#: original_texis/msgattrib.texi:231
#, no-wrap
msgid "-n"
msgstr "-n"

#. type: itemx
#: original_texis/msgattrib.texi:232
#, no-wrap
msgid "--add-location=@var{type}"
msgstr "--add-location=@var{type}"

#. type: opindex
#: original_texis/msgattrib.texi:233
#, no-wrap
msgid "-n@r{, @code{msgattrib} option}"
msgstr "-n@r{, @code{msgattrib} option}"

#. type: opindex
#: original_texis/msgattrib.texi:234
#, no-wrap
msgid "--add-location@r{, @code{msgattrib} option}"
msgstr "--add-location@r{, @code{msgattrib} option}"

#. type: table
#: original_texis/msgattrib.texi:236
msgid "Generate @samp{#: @var{filename}:@var{line}} lines (default)."
msgstr "@samp{#: @var{filename}:@var{line}}という行を生成します(デフォルト)。"

#. type: table
#: original_texis/msgattrib.texi:242
msgid "The optional @var{type} can be either @samp{full}, @samp{file}, or @samp{never}.  If it is not given or @samp{full}, it generates the lines with both file name and line number.  If it is @samp{file}, the line number part is omitted.  If it is @samp{never}, it completely suppresses the lines (same as @code{--no-location})."
msgstr "@var{type}はオプションで、@samp{full}、@samp{file}、または@samp{never}を指定できます。オプションが指定されない、または@samp{full}の場合は、ファイル名と行番号のの両方が生成されます。@samp{file}の場合、行番号は省略されます。@samp{never}の場合は、完全にこの行を抑制します(@code{--no-location}と同じです)。"

#. type: item
