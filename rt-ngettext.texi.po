#. type: pindex
#: original_texis/rt-ngettext.texi:5
#, no-wrap
msgid "ngettext"
msgstr "ngettext"

#. type: cindex
#: original_texis/rt-ngettext.texi:6
#, no-wrap
msgid "@code{ngettext} program, usage"
msgstr "@code{ngettext} program, usage"

#. type: example
#: original_texis/rt-ngettext.texi:9
#, no-wrap
msgid "ngettext [@var{option}] [@var{textdomain}] @var{msgid} @var{msgid-plural} @var{count}\n"
msgstr "ngettext [@var{option}] [@var{textdomain}] @var{msgid} @var{msgid-plural} @var{count}\n"

#. type: cindex
#: original_texis/rt-ngettext.texi:11
#, no-wrap
msgid "lookup plural message translation"
msgstr "lookup plural message translation"

#. type: Plain text
#: original_texis/rt-ngettext.texi:14
msgid "The @code{ngettext} program displays the native language translation of a textual message whose grammatical form depends on a number."
msgstr "@code{ngettext}は数に依存した文法をもつテキストメッセージを母国語に翻訳して表示するプログラムです。"

#. type: item
#: original_texis/rt-ngettext.texi:18
#, no-wrap
msgid "-c @var{context}"
msgstr "-c @var{context}"

#. type: itemx
#: original_texis/rt-ngettext.texi:19
#, no-wrap
msgid "--context=@var{context}"
msgstr "--context=@var{context}"

#. type: opindex
#: original_texis/rt-ngettext.texi:20
#, no-wrap
msgid "-c@r{, @code{ngettext} option}"
msgstr "-c@r{, @code{ngettext} option}"

#. type: opindex
#: original_texis/rt-ngettext.texi:21
#, no-wrap
msgid "--context@r{, @code{ngettext} option}"
msgstr "--context@r{, @code{ngettext} option}"

#. type: table
#: original_texis/rt-ngettext.texi:24
msgid "Specify the context for the messages to be translated.  See @ref{Contexts} for details."
msgstr "翻訳されるメッセージのコンテキストを指定します。詳細は@ref{Contexts}を参照してください。"

#. type: item
#: original_texis/rt-ngettext.texi:25
#, no-wrap
msgid "-d @var{textdomain}"
msgstr "-d @var{textdomain}"

#. type: itemx
#: original_texis/rt-ngettext.texi:26
#, no-wrap
msgid "--domain=@var{textdomain}"
msgstr "--domain=@var{textdomain}"

#. type: opindex
#: original_texis/rt-ngettext.texi:27
#, no-wrap
msgid "-d@r{, @code{ngettext} option}"
msgstr "-d@r{, @code{ngettext} option}"

#. type: opindex
#: original_texis/rt-ngettext.texi:28
#, no-wrap
msgid "--domain@r{, @code{ngettext} option}"
msgstr "--domain@r{, @code{ngettext} option}"

#. type: table
#: original_texis/rt-ngettext.texi:31
msgid "Retrieve translated messages from @var{textdomain}.  Usually a @var{textdomain} corresponds to a package, a program, or a module of a program."
msgstr "@var{textdomain}から翻訳されたメッセージを取得します。@var{textdomain}は通常、パッケージか、プログラムやプログラムのモジュールと一致します。"

#. type: item
#: original_texis/rt-ngettext.texi:32
#, no-wrap
msgid "-e"
msgstr "-e"

#. type: opindex
#: original_texis/rt-ngettext.texi:33
#, no-wrap
msgid "-e@r{, @code{ngettext} option}"
msgstr "-e@r{, @code{ngettext} option}"

#. type: table
#: original_texis/rt-ngettext.texi:39
msgid "Enable expansion of some escape sequences.  This option is for compatibility with the @samp{gettext} program.  The escape sequences @samp{\\a}, @samp{\\b}, @samp{\\c}, @samp{\\f}, @samp{\\n}, @samp{\\r}, @samp{\\t}, @samp{\\v}, @samp{\\\\}, and @samp{\\} followed by one to three octal digits, are interpreted like the System V @samp{echo} program did."
msgstr "いくつかのエスケープシーケンスの展開を可能にします。これは@samp{gettext}プログラムにたいする互換のためのオプションです。対象となるエスケープシーケンスは、@samp{\\a}、@samp{\\b}、@samp{\\c}、@samp{\\f}、@samp{\\n}、@samp{\\r}、@samp{\\t}、@samp{\\v}、@samp{\\\\}、および@samp{\\}と3桁以内の8進数です(System Vの@samp{echo}と同様に処理されます)。"

#. type: item
#: original_texis/rt-ngettext.texi:40
#, no-wrap
msgid "-E"
msgstr "-E"

#. type: opindex
#: original_texis/rt-ngettext.texi:41
#, no-wrap
msgid "-E@r{, @code{ngettext} option}"
msgstr "-E@r{, @code{ngettext} option}"

#. type: table
#: original_texis/rt-ngettext.texi:44
msgid "This option is only for compatibility with the @samp{gettext} program.  It has no effect."
msgstr "このオプションは@samp{gettext}プログラムとの互換性のためだけのもので、何の効果も及ぼしません。"

#. type: item
#: original_texis/rt-ngettext.texi:45
#, no-wrap
msgid "-h"
msgstr "-h"

#. type: itemx
#: original_texis/rt-ngettext.texi:46
#, no-wrap
msgid "--help"
msgstr "--help"

#. type: opindex
#: original_texis/rt-ngettext.texi:47
#, no-wrap
msgid "-h@r{, @code{ngettext} option}"
msgstr "-h@r{, @code{ngettext} option}"

#. type: opindex
#: original_texis/rt-ngettext.texi:48
#, no-wrap
msgid "--help@r{, @code{ngettext} option}"
msgstr "--help@r{, @code{ngettext} option}"

#. type: table
#: original_texis/rt-ngettext.texi:50
msgid "Display this help and exit."
msgstr "このヘルプを表示して終了します。"

#. type: item
#: original_texis/rt-ngettext.texi:51
#, no-wrap
msgid "-V"
msgstr "-V"

#. type: itemx
#: original_texis/rt-ngettext.texi:52
#, no-wrap
msgid "--version"
msgstr "--version"

#. type: opindex
#: original_texis/rt-ngettext.texi:53
#, no-wrap
msgid "-V@r{, @code{ngettext} option}"
msgstr "-V@r{, @code{ngettext} option}"

#. type: opindex
#: original_texis/rt-ngettext.texi:54
#, no-wrap
msgid "--version@r{, @code{ngettext} option}"
msgstr "--version@r{, @code{ngettext} option}"

#. type: table
#: original_texis/rt-ngettext.texi:56
msgid "Output version information and exit."
msgstr "バージョン情報を表示して終了します。"

#. type: var{#1}
#: original_texis/rt-ngettext.texi:57
#, no-wrap
msgid "textdomain"
msgstr "textdomain"

#. type: table
#: original_texis/rt-ngettext.texi:59
msgid "Retrieve translated message from @var{textdomain}."
msgstr "@var{textdomain}から、翻訳されたメッセージを取得します。"

#. type: item
#: original_texis/rt-ngettext.texi:60
#, no-wrap
msgid "@var{msgid} @var{msgid-plural}"
msgstr "@var{msgid} @var{msgid-plural}"

#. type: table
#: original_texis/rt-ngettext.texi:62
msgid "Translate @var{msgid} (English singular) / @var{msgid-plural} (English plural)."
msgstr "msgid( Englishのsingular )、およびmsgid-plural(Englishのplural)を翻訳します。"

#. type: var{#1}
#: original_texis/rt-ngettext.texi:63
#, no-wrap
msgid "count"
msgstr "count"

#. type: table
#: original_texis/rt-ngettext.texi:65
msgid "Choose singular/plural form based on this value."
msgstr "この値にもとづいて、singularとpluralのどちらを使うか選択します。"

#. type: Plain text
#: original_texis/rt-ngettext.texi:71
msgid "If the @var{textdomain} parameter is not given, the domain is determined from the environment variable @code{TEXTDOMAIN}.  If the message catalog is not found in the regular directory, another location can be specified with the environment variable @code{TEXTDOMAINDIR}."
msgstr "パラメーター@var{textdomain}を指定しなかった場合、環境変数@code{TEXTDOMAIN}によりdomainが決定されます。メッセージカタログが標準のディレクトリーで見つからなかった場合には、環境変数@code{TEXTDOMAINDIR}で他の場所を指定できます。"
