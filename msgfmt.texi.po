#: original_texis/msgfmt.texi:110 original_texis/msgfmt.texi:140
#, no-wrap
msgid "--resource@r{, @code{msgfmt} option}"
msgstr "--resource@r{, @code{msgfmt} option}"

#. type: table
#: original_texis/msgfmt.texi:112 original_texis/msgfmt.texi:142
msgid "Specify the resource name."
msgstr "リソース名を指定します。"

#. type: item
#: original_texis/msgfmt.texi:113 original_texis/msgfmt.texi:143
#: original_texis/msgfmt.texi:163 original_texis/msgfmt.texi:193
#: original_texis/msgfmt.texi:244
#, no-wrap
msgid "-l @var{locale}"
msgstr "-l @var{locale}"

#. type: itemx
#: original_texis/msgfmt.texi:114 original_texis/msgfmt.texi:144
#: original_texis/msgfmt.texi:164 original_texis/msgfmt.texi:194
#: original_texis/msgfmt.texi:245
#, no-wrap
msgid "--locale=@var{locale}"
msgstr "--locale=@var{locale}"

#. type: opindex
#: original_texis/msgfmt.texi:115 original_texis/msgfmt.texi:145
#: original_texis/msgfmt.texi:165 original_texis/msgfmt.texi:195
#: original_texis/msgfmt.texi:246
#, no-wrap
msgid "-l@r{, @code{msgfmt} option}"
msgstr "-l@r{, @code{msgfmt} option}"

#. type: opindex
#: original_texis/msgfmt.texi:116 original_texis/msgfmt.texi:146
#: original_texis/msgfmt.texi:166 original_texis/msgfmt.texi:196
#: original_texis/msgfmt.texi:247
#, no-wrap
msgid "--locale@r{, @code{msgfmt} option}"
msgstr "--locale@r{, @code{msgfmt} option}"

#. type: table
#: original_texis/msgfmt.texi:119 original_texis/msgfmt.texi:149
#: original_texis/msgfmt.texi:169 original_texis/msgfmt.texi:199
#: original_texis/msgfmt.texi:250
msgid "Specify the locale name, either a language specification of the form @var{ll} or a combined language and country specification of the form @var{ll_CC}."
msgstr "locale名を指定します。@var{ll}形式による言語指定と、国と言語指定を組み合わせた@var{ll_CC}のどちらでも指定できます。"

#. type: item
#: original_texis/msgfmt.texi:120 original_texis/msgfmt.texi:150
#: original_texis/msgfmt.texi:170 original_texis/msgfmt.texi:200
#: original_texis/msgfmt.texi:251
#, no-wrap
msgid "-d @var{directory}"
msgstr "-d @var{directory}"

#. type: opindex
#: original_texis/msgfmt.texi:121 original_texis/msgfmt.texi:151
#: original_texis/msgfmt.texi:171 original_texis/msgfmt.texi:201
#: original_texis/msgfmt.texi:252
#, no-wrap
msgid "-d@r{, @code{msgfmt} option}"
msgstr "-d@r{, @code{msgfmt} option}"

#. type: table
#: original_texis/msgfmt.texi:123
msgid "Specify the base directory of classes directory hierarchy."
msgstr "classのディレクトリー階層のベースとなるディレクトリーを指定します。"

#. type: item
#: original_texis/msgfmt.texi:124
#, no-wrap
msgid "--source"
msgstr "--source"

#. type: opindex
#: original_texis/msgfmt.texi:125
#, no-wrap
msgid "--source@r{, @code{msgfmt} option}"
msgstr "--source@r{, @code{msgfmt} option}"

#. type: table
#: original_texis/msgfmt.texi:127
msgid "Produce a .java source file, instead of a compiled .class file."
msgstr "コンパイルされた.classファイルのかわりに、.javaソースファイルを生成します。"

#. type: Plain text
#: original_texis/msgfmt.texi:133
msgid "The class name is determined by appending the locale name to the resource name, separated with an underscore.  The @samp{-d} option is mandatory.  The class is written under the specified directory."
msgstr "クラス名はリソース名の後ろに区切り文字のアンダースコアーとlocale名を付加して決定されます。@samp{-d}オプションは必須です。クラスは指定されたディレクトリーに出力されます。"

#. type: subsection
#: original_texis/msgfmt.texi:134
#, no-wrap
msgid "Output file location in C# mode"
msgstr "Output file location in C# mode"

#. type: table
#: original_texis/msgfmt.texi:153
msgid "Specify the base directory for locale dependent @file{.dll} files."
msgstr "localeに依存する@file{.dll}ファイルを出力するベースディレクトリーを指定します。"

#. type: Plain text
#: original_texis/msgfmt.texi:159
msgid "The @samp{-l} and @samp{-d} options are mandatory.  The @file{.dll} file is written in a subdirectory of the specified directory whose name depends on the locale."
msgstr "\\@samp{-l}と@samp{-d}が必須オプションです。@file{.dll}ファイルは、localeに依存した名前の指定ディレクトリーのサブディレクトリーに出力されます。"

#. type: subsection
#: original_texis/msgfmt.texi:160
#, no-wrap
msgid "Output file location in Tcl mode"
msgstr "Output file location in Tcl mode"

#. type: table
#: original_texis/msgfmt.texi:173
msgid "Specify the base directory of @file{.msg} message catalogs."
msgstr "メッセージカタログ@file{.msg}のベースディレクトリーを指定します。"

#. type: Plain text
#: original_texis/msgfmt.texi:178
msgid "The @samp{-l} and @samp{-d} options are mandatory.  The @file{.msg} file is written in the specified directory."
msgstr "@samp{-l}と@samp{-d}は必須オプションです。@file{.msg}は指定されたディレクトリーに出力されます。"

#. type: subsection
#: original_texis/msgfmt.texi:179
#, no-wrap
msgid "Desktop Entry mode operations"
msgstr "Desktop Entry mode operations"

#. type: item
#: original_texis/msgfmt.texi:182 original_texis/msgfmt.texi:233
#, no-wrap
msgid "--template=@var{template}"
msgstr "--template=@var{template}"

#. type: opindex
#: original_texis/msgfmt.texi:183 original_texis/msgfmt.texi:234
#, no-wrap
msgid "--template@r{, @code{msgfmt} option}"
msgstr "--template@r{, @code{msgfmt} option}"

#. type: table
#: original_texis/msgfmt.texi:185
msgid "Specify a .desktop file used as a template."
msgstr "テンプレートとして使用された.desktopファイルを指定します。"

#. type: item
#: original_texis/msgfmt.texi:186
#, no-wrap
msgid "-k[@var{keywordspec}]"
msgstr "-k[@var{keywordspec}]"

#. type: itemx
#: original_texis/msgfmt.texi:187
#, no-wrap
msgid "--keyword[=@var{keywordspec}]"
msgstr "--keyword[=@var{keywordspec}]"

#. type: opindex
#: original_texis/msgfmt.texi:188
#, no-wrap
msgid "-k@r{, @code{msgfmt} option}"
msgstr "-k@r{, @code{msgfmt} option}"

#. type: opindex
#: original_texis/msgfmt.texi:189
#, no-wrap
msgid "--keyword@r{, @code{msgfmt} option}"
msgstr "--keyword@r{, @code{msgfmt} option}"

#. type: table
#: original_texis/msgfmt.texi:192
msgid "Specify @var{keywordspec} as an additional keyword to be looked for.  Without a @var{keywordspec}, the option means to not use default keywords."
msgstr "探すべき追加のキーワードとして、@var{keywordspec}を指定します。@var{keywordspec}を指定しない場合は、デフォルトのキーワードを使用しないことを意味します。"

#. type: table
#: original_texis/msgfmt.texi:204
msgid "Specify the directory where PO files are read.  The directory must contain the @samp{LINGUAS} file."
msgstr "POファイルが読み込まれるディレクトリーを指定します。このディレクトリーは、@samp{LINGUAS}ファイルを含んでいなければなりません。"

#. type: Plain text
#: original_texis/msgfmt.texi:209
msgid "To generate a @samp{.desktop} file for a single locale, you can use it as follows."
msgstr "単一のlocaleにたいして@samp{.desktop}ファイルを生成するために、以下を使用することができます。"

#. type: example
#: original_texis/msgfmt.texi:213
#, no-wrap
msgid ""
"msgfmt --desktop --template=@var{template} --locale=@var{locale} \\\n"
"  -o @var{file} @var{filename}.po @dots{}\n"
msgstr ""
"msgfmt --desktop --template=@var{template} --locale=@var{locale} \\\n"
"  -o @var{file} @var{filename}.po @dots{}\n"

#. type: Plain text
#: original_texis/msgfmt.texi:217 original_texis/msgfmt.texi:266
msgid "msgfmt provides a special \"bulk\" operation mode to process multiple @file{.po} files at a time."
msgstr "1度に複数の@file{.po}ファイルを処理するために、msgfmtは特別な\"bulk\"モードを提供します。"

#. type: example
#: original_texis/msgfmt.texi:220
#, no-wrap
msgid "msgfmt --desktop --template=@var{template} -d @var{directory} -o @var{file}\n"
msgstr "msgfmt --desktop --template=@var{template} -d @var{directory} -o @var{file}\n"

#. type: Plain text
#: original_texis/msgfmt.texi:226 original_texis/msgfmt.texi:275
msgid "msgfmt first reads the @samp{LINGUAS} file under @var{directory}, and then processes all @samp{.po} files listed there.  You can also limit the locales to a subset, through the @samp{LINGUAS} environment variable."
msgstr "最初にmsgfmtは@var{directory}配下の@samp{LINGUAS}ファイルを読み込み、そこにリストされたすべての@samp{.po}ファイルを処理します。@samp{LINGUAS}環境変数を通じて、localeをサブセットに制限することもできます。"

#. type: Plain text
#: original_texis/msgfmt.texi:229 original_texis/msgfmt.texi:278
msgid "For either operation modes, the @samp{-o} and @samp{--template} options are mandatory."
msgstr "どちらの操作モードでも、@samp{-o}と@samp{--template}のオプションは必須です。"

#. type: subsection
#: original_texis/msgfmt.texi:230
#, no-wrap
msgid "XML mode operations"
msgstr "XML mode operations"

#. type: table
#: original_texis/msgfmt.texi:236
msgid "Specify an XML file used as a template."
msgstr "テンプレートとして使用されるXMLファイルを指定します。"

#. type: item
#: original_texis/msgfmt.texi:237
#, no-wrap
msgid "-L @var{name}"
msgstr "-L @var{name}"

#. type: itemx
#: original_texis/msgfmt.texi:238
#, no-wrap
