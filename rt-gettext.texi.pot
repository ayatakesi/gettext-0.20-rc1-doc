msgstr ""

#. type: table
#: original_texis/rt-gettext.texi:25
msgid ""
"Specify the context for the messages to be translated.  See @ref{Contexts} "
"for details."
msgstr ""

#. type: item
#: original_texis/rt-gettext.texi:26
#, no-wrap
msgid "-d @var{textdomain}"
msgstr ""

#. type: itemx
#: original_texis/rt-gettext.texi:27
#, no-wrap
msgid "--domain=@var{textdomain}"
msgstr ""

