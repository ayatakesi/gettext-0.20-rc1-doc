#, no-wrap
msgid "--properties-input"
msgstr "--properties-input"

#. type: opindex
#: original_texis/msgcmp.texi:72
#, no-wrap
msgid "-P@r{, @code{msgcmp} option}"
msgstr "-P@r{, @code{msgcmp} option}"

#. type: opindex
#: original_texis/msgcmp.texi:73
#, no-wrap
msgid "--properties-input@r{, @code{msgcmp} option}"
msgstr "--properties-input@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:76
msgid "Assume the input files are Java ResourceBundles in Java @code{.properties} syntax, not in PO file syntax."
msgstr "入力ファイルがPOファイルの構文ではなく、Javaの@code{.properties}の構文にのっとったJava ResourceBundleファイルだとみなします。"

#. type: item
#: original_texis/msgcmp.texi:77
#, no-wrap
msgid "--stringtable-input"
msgstr "--stringtable-input"

#. type: opindex
#: original_texis/msgcmp.texi:78
#, no-wrap
msgid "--stringtable-input@r{, @code{msgcmp} option}"
msgstr "--stringtable-input@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:81
msgid "Assume the input files are NeXTstep/GNUstep localized resource files in @code{.strings} syntax, not in PO file syntax."
msgstr "入力ファイルがPOファイルの構文ではなく、NeXTstep/GNUstepのlocalized resourceの@code{.strings}の構文にのっとったファイルだとみなします。"

