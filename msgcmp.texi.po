#. type: pindex
#: original_texis/msgcmp.texi:5
#, no-wrap
msgid "msgcmp"
msgstr "msgcmp"

#. type: cindex
#: original_texis/msgcmp.texi:6
#, no-wrap
msgid "@code{msgcmp} program, usage"
msgstr "@code{msgcmp} program, usage"

#. type: example
#: original_texis/msgcmp.texi:9
#, no-wrap
msgid "msgcmp [@var{option}] @var{def}.po @var{ref}.pot\n"
msgstr "msgcmp [@var{option}] @var{def}.po @var{ref}.pot\n"

#. type: cindex
#: original_texis/msgcmp.texi:11
#, no-wrap
msgid "compare PO files"
msgstr "compare PO files"

#. type: Plain text
#: original_texis/msgcmp.texi:19
msgid "The @code{msgcmp} program compares two Uniforum style .po files to check that both contain the same set of msgid strings.  The @var{def}.po file is an existing PO file with the translations.  The @var{ref}.pot file is the last created PO file, or a PO Template file (generally created by @code{xgettext}).  This is useful for checking that you have translated each and every message in your program.  Where an exact match cannot be found, fuzzy matching is used to produce better diagnostics."
msgstr "@code{msgcmp}プログラムは、Uniforum形式の2つの.poファイルを比較して、同じmsgid文字列を含んでいるかチェックするプログラムです。@var{def}.poファイルは、翻訳を含んだ既存のPOファイルです。@var{ref}.potは、最後に作成したPOファイル、またはPO Templateファイル(通常@code{xgettext}により作成される)です。このプログラムは、プログラム内のメッセージが翻訳されているかチェックするときに便利です。完全に一致するエントリーが見つからない場合は、より良い診断メッセージを生成するためにfuzzyマッチングが行われます。"

#. type: subsection
#: original_texis/msgcmp.texi:20
#, no-wrap
msgid "Input file location"
msgstr "Input file location"

#. type: item
#: original_texis/msgcmp.texi:23
#, no-wrap
msgid "@var{def}.po"
msgstr "@var{def}.po"

#. type: table
#: original_texis/msgcmp.texi:25
msgid "Translations."
msgstr "翻訳です。"

#. type: item
#: original_texis/msgcmp.texi:26
#, no-wrap
msgid "@var{ref}.pot"
msgstr "@var{ref}.pot"

#. type: table
#: original_texis/msgcmp.texi:28
msgid "References to the sources."
msgstr "ソースへの参照です。"

#. type: item
#: original_texis/msgcmp.texi:29
#, no-wrap
msgid "-D @var{directory}"
msgstr "-D @var{directory}"

#. type: itemx
#: original_texis/msgcmp.texi:30
#, no-wrap
msgid "--directory=@var{directory}"
msgstr "--directory=@var{directory}"

#. type: opindex
#: original_texis/msgcmp.texi:31
#, no-wrap
msgid "-D@r{, @code{msgcmp} option}"
msgstr "-D@r{, @code{msgcmp} option}"

#. type: opindex
#: original_texis/msgcmp.texi:32
#, no-wrap
msgid "--directory@r{, @code{msgcmp} option}"
msgstr "--directory@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:35
msgid "Add @var{directory} to the list of directories.  Source files are searched relative to this list of directories."
msgstr "ディレクトリーのリストに@var{directory}を追加します。このディレクトリーのリストよりソースファイルを検索します。"

#. type: subsection
#: original_texis/msgcmp.texi:38
#, no-wrap
msgid "Operation modifiers"
msgstr "Operation modifiers"

#. type: item
#: original_texis/msgcmp.texi:41
#, no-wrap
msgid "-m"
msgstr "-m"

#. type: itemx
#: original_texis/msgcmp.texi:42
#, no-wrap
msgid "--multi-domain"
msgstr "--multi-domain"

#. type: opindex
#: original_texis/msgcmp.texi:43
#, no-wrap
msgid "-m@r{, @code{msgcmp} option}"
msgstr "-m@r{, @code{msgcmp} option}"

#. type: opindex
#: original_texis/msgcmp.texi:44
#, no-wrap
msgid "--multi-domain@r{, @code{msgcmp} option}"
msgstr "--multi-domain@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:46
msgid "Apply @var{ref}.pot to each of the domains in @var{def}.po."
msgstr "@var{def}.po 内の各ドメインにたいして@var{ref}.potを適用します。"

#. type: item
#: original_texis/msgcmp.texi:47
#, no-wrap
msgid "-N"
msgstr "-N"

#. type: itemx
#: original_texis/msgcmp.texi:48
#, no-wrap
msgid "--no-fuzzy-matching"
msgstr "--no-fuzzy-matching"

#. type: opindex
#: original_texis/msgcmp.texi:49
#, no-wrap
msgid "-N@r{, @code{msgcmp} option}"
msgstr "-N@r{, @code{msgcmp} option}"

#. type: opindex
#: original_texis/msgcmp.texi:50
#, no-wrap
msgid "--no-fuzzy-matching@r{, @code{msgcmp} option}"
msgstr "--no-fuzzy-matching@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:53
msgid "Do not use fuzzy matching when an exact match is not found.  This may speed up the operation considerably."
msgstr "完全に一致するものが見つからない場合、fuzzyマッチングを行いません。これにより処理のスピードが大幅に改善されます。"

#. type: item
#: original_texis/msgcmp.texi:54
#, no-wrap
msgid "--use-fuzzy"
msgstr "--use-fuzzy"

#. type: opindex
#: original_texis/msgcmp.texi:55
#, no-wrap
msgid "--use-fuzzy@r{, @code{msgcmp} option}"
msgstr "--use-fuzzy@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:59
msgid "Consider fuzzy messages in the @var{def}.po file like translated messages.  Note that using this option is usually wrong, because fuzzy messages are exactly those which have not been validated by a human translator."
msgstr "@var{def}.po内のfuzzyメッセージを、翻訳されたメッセージとみなします。fuzzyメッセージは翻訳者により検証されていないので、普通はこのオプションを使うのは正しくないことに注意してください。"

#. type: item
#: original_texis/msgcmp.texi:60
#, no-wrap
msgid "--use-untranslated"
msgstr "--use-untranslated"

#. type: opindex
#: original_texis/msgcmp.texi:61
#, no-wrap
msgid "--use-untranslated@r{, @code{msgcmp} option}"
msgstr "--use-untranslated@r{, @code{msgcmp} option}"

#. type: table
#: original_texis/msgcmp.texi:64
msgid "Consider untranslated messages in the @var{def}.po file like translated messages.  Note that using this option is usually wrong."
msgstr "@var{def}.po内の未翻訳のメッセージを、翻訳されたメッセージとみなします。普通はこのオプションを使うのは正しくないことに注意してください。"

